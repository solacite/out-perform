script/source = "extends Camera2D

@export var shake_enabled: bool = false
@export var shake_intensity: float = 5.0

var original_position: Vector2

func _ready():
	original_position = position
	add_to_group(\"camera\")

func _process(delta):
	if shake_enabled:
		var shake_x = randf_range(-shake_intensity, shake_intensity)
		var shake_y = randf_range(-shake_intensity, shake_intensity)
		position = original_position + Vector2(shake_x, shake_y)
	else:
		position = original_position

func start_shake():
	shake_enabled = true
	print(\"Camera shake started\")

func stop_shake():
	shake_enabled = false
	position = original_position
	print(\"Camera shake stopped\")

func set_intensity(intensity: float):
	shake_intensity = intensity
"

